{namespace bitex.templates}

/**
  * @param permissions
  */
{template .AlgoPermissionsDialogContent}
  <p class="lead"> {msg desc="Algorithm trading request permmissions dialog content"}This algorithm is requesting your permission to:{/msg}</p>

  <ul class="list-unstyled" style="line-height: 2">
  {foreach $permission in  $permissions }
    <li><span class="fa fa-check text-success"></span>
    {switch $permission}
      {case 'notification'}
        {msg desc="Permission in Algo Permissions Dialog"} Show notifications{/msg}
      {case 'balance'}
        {msg desc="Permission in Algo Permissions Dialog"} See your balances{/msg}
      {case 'execution_report'}
        {msg desc="Permission in Algo Permissions Dialog"} Listen to the execution reports from your orders{/msg}
      {case 'new_order_limited'}
        {msg desc="Permission in Algo Permissions Dialog"} Send limited orders{/msg}
      {case 'new_buy_order_limited'}
        {msg desc="Permission in Algo Permissions Dialog"} Send buy limited orders{/msg}
      {case 'new_sell_order_limited'}
        {msg desc="Permission in Algo Permissions Dialog"} Send sell limited orders{/msg}
      {case 'new_market_order'}
        {msg desc="Permission in Algo Permissions Dialog"} Send market orders{/msg}
      {case 'new_buy_market_order'}
        {msg desc="Permission in Algo Permissions Dialog"} Send buy market orders{/msg}
      {case 'new_sell_market_order'}
        {msg desc="Permission in Algo Permissions Dialog"} Send sell market order{/msg}
      {case 'cancel_order'}
        {msg desc="Permission in Algo Permissions Dialog"} Cancel orders{/msg}
      {default}
        {$permission}
    {/switch}
    </li>
  {/foreach}
  </ul>
{/template}


/**
  * @param msg_customer_detail
  */
{template .CustomerDetailDialog}
  <div class="row">
    <div class="span6">
      {msg desc="Customer username"}Username:{/msg}{$msg_customer_detail['Username']}
    </div>
  </div>
{/template}

/**
  * @param error_message
  * @param error_code
  */
{template .ErrorDialogContent}
  <p class="lead">{$error_message}</p>
  <dl class="dl-horizontal">
     <dt>{msg desc="Error code label on error dialog content"}Error code{/msg}</dt>
     <dd>{$error_code}</dd>
  </dl>
{/template}

/**
  * @param filename
  */
{template .FileViewDialogContent}
  <img src="{$filename}" style="width:500px"/>
{/template}

/**
  * @param depositDetailData
  * @param depositReceiptList
  */
{template .DepositDetailDialogContent}
  <table class="table table-bordered table-condensed">
    <tbody>
      {foreach $data in $depositDetailData}
        <tr>
          <td>{{$data['key']}}{sp}:</td>
          <td>{{$data['value']}}</td>
        </tr>
      {/foreach}
      {if $depositReceiptList} 
        <tr>
          <td>Recibo:</td>
          <td>
          {foreach $receipt in $depositReceiptList}
            <a href="{{$receipt['url']}}" target="_blank">{msg desc="Download picture link on deposit receipt dialog content"}Download{/msg}</a>{sp}
          {/foreach}
          </td>
        </tr>

        {foreach $receipt in $depositReceiptList}
          <tr>
            <td colspan="2">
              {if $receipt['type'] == 'image' }
                <img width="100%" src="{{$receipt['url']}}"/>
              {else}
                {if $receipt['type'] == 'pdf' }
                  <iframe src="https://docs.google.com/gview?url={{$receipt['url']}}&embedded=true" style="width:515px; height:300px;" frameborder="0" />
                {/if}
              {/if}
            </td>
          </tr>
        {/foreach}
      {/if}

    </tbody>
  </table>
{/template}


/**
  * @param depositReceiptList
  */
{template .DepositReceiptDialogContent}
  {foreach $receipt_url in $depositReceiptList}
    <img width="100%" src="{{$receipt_url}}"/>
    <a href="{{$receipt_url}}" target="_blank">{msg desc="Download picture link on deposit receipt dialog content"}Download{/msg}</a>
  {/foreach}
{/template}

/**
  * @param id_value
  * @param percentFeeID
  * @param fixedFeeID
  * @param totalFeesID
  * @param netValueID
  * @param controlNumber
  * @param currencySign
  * @param value
  * @param percentFee
  * @param fixedFee
  */
{template .BrokerConfirmDepositContent}
  <form class="form-horizontal broker-confirm-deposit">
    <div class="control-group">
      <label class="control-label">{msg desc="Control number label"}Control number{/msg}</label>
      <div class="controls">
        <label class="control-label" style="text-align: left;">{$controlNumber}</label>
      </div>
    </div>
    <div class="control-group">
      <label class="control-label">{msg desc="Amount label"}Amount paid{/msg}</label>
      <div class="controls">
        <div class="input-prepend">
          <span class="add-on">{$currencySign}</span>
          <input id="{$id_value}" type="text" class="input-small" size="16" name="PaidValue" value="{$value}">
        </div>
      </div>
    </div>
    <div class="control-group">
      <label class="control-label">{msg desc="Fee percentage label"}Fees:{/msg}</label>
      <div class="controls">
        <div class="row-fluid">
          <div class="span4">
            <div class="input-append">
              <input id="{$percentFeeID}" type="text" class="input-mini" size="16" name="Fee" value="{$percentFee}">
              <span class="add-on">%</span>
            </div>
          </div>

          <div class="span4">
            <div class="input-prepend">
              <span class="add-on">{$currencySign}</span>
              <input id="{$fixedFeeID}" type="text" class="input-mini" size="16" name="Fee" value="{$fixedFee}">
            </div>
          </div>

          <div class="span4">
            <label id="{$totalFeesID}" class="control-label broker-confirm-deposit-fee" style="text-align: left;"></label>
          </div>
        </div>
      </div>
    </div>

    <div class="control-group">
      <label class="control-label">{msg desc="Net amount"}Net amount{/msg}</label>
      <div class="controls">
        <label id="{$netValueID}" class="control-label broker-confirm-deposit-net-amount" style="text-align: left;"></label>
        <input id="{$netValueID}_value" type="hidden" value="">
      </div>
    </div>

  </form>
{/template}

/**
  * @param variables
  * @param pattern
  * @param formula
  * @param value
  */
{template .InstantFiatDepositDialogContent}
  <div class="row-fluid" data-deposit-status="confirm">
      <p>{msg desc="Instant deposit fiat description 1"}Net value:{/msg}{sp}
        <span class="bitex-model"
              data-model-key-list="{$variables}"
              data-model-formula="min( {$formula},  {$value} )"
              data-model-formatter="currency"
              data-model-formatter-pattern="{$pattern}"
              data-blink-class="balance-info-blink"></span>
      </p>

      <p>{msg desc="Instant deposit fiat description 2"}Pre approval limit:{/msg}
            <span class="bitex-model"
                    data-model-key-list="{$variables}"
                    data-model-formula="{$formula}"
                    data-model-formatter="currency"
                    data-model-formatter-pattern="{$pattern}"
                    data-blink-class="balance-info-blink">
            </span>{sp}{msg desc="Instant deposit fiat description 3"}(3x the approx. value of your account.){/msg}
      </p>

      <p>{msg desc="Instant deposit fiat description 4"}Pre approval fee: 0% (Promotional){/msg}</p>

      <div class="well">
          <p>
              {msg desc="Instant deposit fiat message"}
                  You won't be able to withdrawal funds until your DEPOSIT IS CONFIRMED by the exchange.
                  In case of a fake deposit receipt or any fraud attempt, an administrative fee
                  of 30% of the deposit amount will be applied and your account will be TERMINATED.
              {/msg}
          </p>
      </div>
  </div>
{/template}

/**
  * @param show_title
  * @param msg_broker
  */
{template .BrokerView}
  {if $show_title}
  <header class="page__head section-title">
    <h1 class="page__title beta">{msg desc="Title for the Broker view"}My broker{/msg}</h1>
  </header>
  {/if}

  <div class="page__body content">
    {if $msg_broker['BrokerID'] == -1 }
    {else}
      <div class="broker-address">
        <h4 class="broker-address__title">
            Address
        </h4>

        <strong>{$msg_broker['BusinessName']}</strong><br/>
        {$msg_broker['Address']}<br/>
        {$msg_broker['City']}, {$msg_broker['State']} {$msg_broker['ZipCode']} - {$msg_broker['Country']}<br/>
        {if $msg_broker['PhoneNumber1']}
          <abbr title="{msg desc="label in broker general information table"}Phone{/msg}">{msg desc="label in broker general information table"}P:{/msg}</abbr>{$msg_broker['PhoneNumber1']} {if $msg_broker['PhoneNumber2']}, {$msg_broker['PhoneNumber2']}{/if}<br/>
        {/if}
        {if $msg_broker['Skype']}
          Skype: {$msg_broker['Skype']}<br/>
        {/if}
        <a href="mailto:{$msg_broker['Email']}" target="_blank">{$msg_broker['Email']}</a>
      </div>

      {if $msg_broker['BrokerID'] > 0 }
        <div class="broker-market">
          <h4 class="broker-market__title">
            {msg desc="Allowed markets section subtitle on broker view"}Markets{/msg}
          </h4>

          <div class="broker-market__markets">
          {foreach $allowed_market in keys($msg_broker['AllowedMarkets'])}
            { $msg_broker['AllowedMarkets'][$allowed_market]['description']}
          {/foreach}
          </div>
        </div>
      {/if}

      {if $msg_broker['BrokerID'] >= 0 }
        {if length($msg_broker['FeeStructure']) > 0 }
        <!-- .broker-fee -->
        <div class="broker-fee">
          <h4 class="broker-fee__title">
            {msg desc="Fee section subtitle on Broker view"}Fees{/msg}
          </h4>

          <table class="broker-fee__table table">
            <thead>
              <tr>
                <th>{msg desc="Operation column on Broker fees table"}Operation{/msg}</th>
                <th>{msg desc="Fees column on Broker fees table"}Fee{/msg}</th>
                <th>{msg desc="Terms column on Broker fees table"}Terms{/msg}</th>
              </tr>
            </thead>
            <tbody>
              {if not $msg_broker['IsBrokerHub'] }
                {if $msg_broker['FormattedTransactionFeeBuy'] == $msg_broker['FormattedTransactionFeeSell']}
                  <tr>
                    <td>{msg desc="label in broker fees table"}Trade fee{/msg}</td>
                    {if $msg_broker['FormattedTakerTransactionFeeBuy'] == $msg_broker['FormattedTransactionFeeBuy']}
                      <td>{$msg_broker['FormattedTransactionFeeBuy']}</td>
                    {else}
                      <td>{$msg_broker['FormattedTakerTransactionFeeBuy']} / {$msg_broker['FormattedTransactionFeeBuy']}</td>
                    {/if}
                    <td></td>
                  </tr>
                {else}
                  <tr>
                    <td>{msg desc="label in broker fees table"}Trade fee - Buy side{/msg}</td>
                    {if $msg_broker['FormattedTakerTransactionFeeBuy'] == $msg_broker['FormattedTransactionFeeBuy']}
                      <td>{$msg_broker['FormattedTransactionFeeBuy']}</td>
                    {else}
                      <td>{$msg_broker['FormattedTakerTransactionFeeBuy']} / {$msg_broker['FormattedTransactionFeeBuy']}</td>
                    {/if}
                    <td></td>
                  </tr>
                  <tr>
                    <td>{msg desc="label in broker fees table"}Trade fee - Sell side{/msg}</td>
                    {if $msg_broker['FormattedTakerTransactionFeeSell'] == $msg_broker['FormattedTransactionFeeSell']}
                      <td>{$msg_broker['FormattedTransactionFeeSell']} %</td>
                    {else}
                      <td>{$msg_broker['FormattedTakerTransactionFeeSell']} / {$msg_broker['FormattedTransactionFeeSell']}</td>
                    {/if}
                    <td></td>
                  </tr>
                {/if}
              {/if}
              {foreach $fee in $msg_broker['FeeStructure']}
                <tr>
                  <td>{$fee['Operation']}</td>
                  <td>{$fee['Fee']}</td>
                  <td>{$fee['Terms']}</td>
                </tr>
              {/foreach}
            </tbody>
          </table>
        </div>
        <!-- /.broker-fee -->
        {/if}
      {/if}

      {if not $msg_broker['IsBrokerHub'] }
      <!-- .broker-wallet -->
      <div class="broker-wallet">
        <h4 class="broker-wallet__title">
          {msg desc="Wallet section subtitle on Broker view"}Wallets{/msg}
        </h4>

        <table class="broker-wallet__table table">
          <thead>
            <tr>
              <th>{msg desc="Coin  column on Broker wallet table"}Coin{/msg}</th>
              <th>{msg desc="type  column on Broker wallet table"}Type{/msg}</th>
              <th>{msg desc="address column type on Broker wallet table"}Address{/msg}</th>
              <th>{msg desc="multisig column type on Broker wallet table"}Multi signature{/msg}</th>
              <th>{msg desc="managed by column on Broker wallet table"}Managed by{/msg}</th>
            </tr>
          </thead>
          <tbody>
            {foreach $crypto_currencies in $msg_broker['CryptoCurrencies']}
              {foreach $wallets in $crypto_currencies['Wallets'] }
                <tr>
                  <td>{$crypto_currencies['CurrencyDescription']}</td>
                  <td>{$wallets['type'] }</td>
                  <td>
                  {if $crypto_currencies['CurrencyCode'] == 'BTC' }
                    {switch $wallets['address'][0]}
                      {case 'm','n','2','9','c'}
                        <a href="https://live.blockcypher.com/btc-testnet/address/{$wallets['address']}/" target="_blank">{$wallets['address'] }</a>
                      {default}
                        <a href="https://blockchain.info/address/{$wallets['address']}/" target="_blank">{$wallets['address'] }</a>
                    {/switch}
                  {else}
                    {$wallets['address'] }
                  {/if}
                  </td>
                  <td>
                    {if $wallets['multisig']}
                      {msg desc="Multisig true"}Yes{/msg}
                    {else}
                      {msg desc="Multisig false"}No{/msg}
                    {/if}
                  </td>
                  <td>{$wallets['managed_by'] }</td>
                </tr>
              {/foreach}
            {/foreach}
          </tbody>
        </table>
      </div>
      <!-- /.broker-wallet -->
      {/if}

      <!-- .broker-terms -->
      <div class="broker-terms">
        <h4 class="broker-terms__title">
          {msg desc="Terms of service section subtitle on Broker view"}Terms of service{/msg}
        </h4>
        <iframe class="broker-terms__iframe" src="{$msg_broker['TosUrl']}"></iframe>
      </div>
      <!-- /.broker-terms -->
    {/if}
  </div>



{/template}

/**
  * @param reason_id
  * @param custom_reason_id
  */
{template .CancelDepositDialogContent}
<p>{msg desc="Cancel deposit dialog message"}Enter a reason for cancelling the deposit{/msg}</p>
<form class="form-horizontal">
  <fieldset>
    <div class="control-group">
      <label class="control-label" for="{$reason_id}" >{msg desc="Cancel deposit reason label"}Reason:{/msg}</label>
      <div class="controls">
        <select id="{$reason_id}">
          <option value="0" selected>{msg desc="reason for cancelling deposit"}Other{/msg}</option>
        </select>
      </div>
    </div>
    <div class="control-group">
      <div class="controls">
        <textarea id="{$custom_reason_id}" rows="2" style=""></textarea>
      </div>
    </div>
  </fieldset>
</form>
{/template}


/**
  */
{template .GoogleAuthenticationCodeDialogContent}
  <form class="form-horizontal" data-deposit-status="prepare" data-uniform-control-holder-class="control-group">
    <fieldset>
      <div class="control-group">
        <label>
          {msg desc="Google Authenticator code dialog label"}Google Authenticator code:{/msg}
        </label>
        <input name="token"
               id="id_input_google_authenticator"
               data-uniform-validators="required"
               data-uniform-label="{msg desc="Limit table field header"}Google authenticator code{/msg}"
               label="{msg desc="Google Authenticator code placeholder"}eg. 555555{/msg}"
               class="input-large"
               type="text"
               size="10">
      </div>
    </fieldset>
  </form>
{/template}

/**
  * @param id
  * @param description
  * @param askForTrust
  */
{template .SecondFactorTokenDialogContent}
  <form class="form-horizontal" data-deposit-status="prepare" data-uniform-control-holder-class="control-group">
    <fieldset>
      <div class="control-group">
        <label>
          {$description}
        </label>
        <input name="token"
               id="{$id}_token"
               data-uniform-validators="required"
               label="{msg desc="Placeholder for the code field on the second factor token dialog"}eg. 5555555{/msg}"
               class="input-large"
               type="text"
               size="10">
      </div>
      {if $askForTrust}
      <div class="control-group">
        <label class="checkbox">
          <input type="checkbox" name="TrustedDevice">{msg desc="Label on the two factor authenticatior"}Trust this device for 30 days{/msg}
        </label>
      </div>
      {/if}
    </fieldset>
  </form>
{/template}


/**
  */
